{
  "expo": {
    "name": "couple-friend-app",
    "slug": "couple-friend-app",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "couplefriendapp",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jagadish182004.couplefriendapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": ["expo-splash-screen"]
  }
}
